{
  "version": 2,
  "builds": [
    {
      "src": "./server.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "server.js": {
      "maxDuration": 60 
    }
  },
  "routes": [
    {
      "src": "/scrape",
      "dest": "/server.js"
    },
    {
      "src": "/auth/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
