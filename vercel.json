{
  "version": 2,
  "builds": [
    {
      "src": "./server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/scrape",
      "dest": "/server.js"
    },
    {
      "src": "auth/dashboard/api/signin",
      "dest": "/server.js"
    },
    {
      "src": "/auth/dashboard",
      "dest": "/server.js"
    },
    {
      "src": "/auth/signout",
      "dest": "/server.js"
    },
    {
      "src": "/auth/session/refresh",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
